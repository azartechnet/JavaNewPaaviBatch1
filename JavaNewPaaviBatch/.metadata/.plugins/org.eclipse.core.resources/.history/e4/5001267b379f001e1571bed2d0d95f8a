
public class Switchp1 {

	public static void main(String[] args) {
		
		int exp1=1;
		switch(exp1)
		{
		case 1:
			System.out.println("Case-1");
			break;
		case 2:
			System.out.println("Case-2");
			break;
		case 3:
			System.out.println("Case-3");
			break;
		default:
			System.out.println("default case");
			break;
		}
	}

}
